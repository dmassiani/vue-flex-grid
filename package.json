{
  "name": "@liqueflies/vue-flex-grid",
  "version": "0.0.1",
  "description": "A powerful flexbox grid system for Vue.js 2.x, based on flexboxgrid and css-modules.",
  "author": {
    "name": "Lorenzo Girardi",
    "url": "https://github.com/liqueflies"
  },
  "homepage": "https://github.com/liqueflies/vue-flex-grid",
  "main": "umd/vue-flex-grid.js",
  "unpkg": "umd/vue-flex-grid.min.js",
  "module": "umd/vue-flex-grid.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run lint && rimraf umd && cross-env BABEL_ENV=rollup rollup -c rollup.config.js",
    "example": "node example/server.js",
    "lint": "eslint src/*.js utils/*.js"
